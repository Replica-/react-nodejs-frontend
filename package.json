{
  "name": "clubdog",
  "private": true,
  "version": "0.0.1",
  "description": "Toro Solutions - ClubDog",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist;copyfiles -f ./src/assets/* ./dist/assets ",
    "copy-dist": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist;copyfiles -f ./src/assets/* ./dist/assets",
    "dist": "npm run copy-dist & webpack --optimize-minimize --env=dist",
    "dist-deploy": "npm run copy-dist & webpack --optimize-minimize --env=dist ; gzip ./dist/assets/app.js --force; mv -f ./dist/assets/app.js.gz ./dist/assets/app.js; aws s3 cp --content-encoding 'gzip' /Volumes/src/cms-CSR/dist/assets s3://shareableapps-cms-staging/csr/assets --recursive --acl public-read",
    "staging-deploy": "npm run copy-dist & webpack --env=dist ; aws s3 cp /Volumes/src/cms-CSR/dist/assets s3://shareableapps-cms-staging/csr/assets --recursive --acl public-read",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "cp -f ./src/index-dev.html ./src/index.html ; node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
